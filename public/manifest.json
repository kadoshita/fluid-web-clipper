{
    "name": "fluid Web Clipper",
    "description": "An application for Web clipping and sharing.",
    "version": "1.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "options_ui": {
        "page": "option.html",
        "open_in_tab": true
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "webRequest"
    ]
}
